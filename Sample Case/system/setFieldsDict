FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
    zoneToCell
    {
        name "cellZoneName";
        fieldValues
        (
            volScalarFieldValue alpha.water 0
        );
    }
 );


